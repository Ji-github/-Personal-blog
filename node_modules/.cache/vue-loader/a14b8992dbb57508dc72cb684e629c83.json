rt default {\n  components: {\n    Layout,\n    BlogDetail,\n    BlogTOC,\n    BlogComment,\n  },\n  mixins: [fetchData(null), mainScroll(\"mainContainer\")],\n  methods: {\n    async fetchData() {\n      const resp = await getBlog(this.$route.params.id);\n      titleController.setRouteTitle(resp.title);\n      return resp;\n    },\n  },\n\n  updated() {\n    const hash = location.hash;\n    location.hash = \"\";\n    setTimeout(() => {\n      location.hash = hash;\n    }, 50);\n  },\n};\n</script>\n\n<style scoped lang=\"less\">\n.main-container {\n  overflow-y: scroll;\n  height: 100%;\n  box-sizing: border-box;\n  padding: 20px;\n  position: relative;\n  width: 100%;\n  overflow-x: hidden;\n  scroll-behavior: smooth;\n}\n.right-container {\n  width: 300px;\n  height: 100%;\n  overflow-y: scroll;\n  box-sizing: border-box;\n  position: relative;\n  padding: 20px;\n}\n</style>\n"]}]}